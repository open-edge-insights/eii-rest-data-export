{
    "SchemaVersion": "0.2",
    "Runtime": "docker",
    "RuntimeOptions": {
        "ContainerImage": "${idx:ia_rest_export:$EII_VERSION}",
        "TemporaryVolumes": [
            "$EII_INSTALL_PATH:$EII_INSTALL_PATH"
        ]
    },
    "DataStore": {
        "Protocol": "DIRECT",
        "DataBuckets": [
            {
                "Name": "EII/RestDataExport",
                "Permissions": "RO"
            },
            {
                "Name": "EII/GlobalEnv",
                "Permissions": "RO"
            },
            {
                "Name": "EII/Publickeys",
                "Permissions": "RO"
            }
        ]
    }
}
